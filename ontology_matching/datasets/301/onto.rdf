<rdf:RDF
    xmlns="http://oaei.ontologymatching.org/2007/benchmarks/301/onto.rdf#"
    xml:base="http://oaei.ontologymatching.org/2007/benchmarks/301/onto.rdf#"
    xmlns:units		="&units;"
    xmlns:foaf		="http://xmlns.com/foaf/0.1/"
