		</owl:Restriction>
	</rdfs:subClassOf>
	<rdfs:subClassOf>
		<owl:Restriction>
			<owl:onProperty rdf:resource="#hasTitle" />
			<owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
		</owl:Restriction>
	</rdfs:subClassOf>
	<rdfs:subClassOf>
		<owl:Restriction>
			<owl:onProperty rdf:resource="#hasSchool" />
			<owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
		</owl:Restriction>
	</rdfs:subClassOf>
	<rdfs:subClassOf>
		<owl:Restriction>
			<owl:onProperty rdf:resource="#hasYear" />
