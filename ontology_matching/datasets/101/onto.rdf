      <owl:Class>
	<owl:unionOf rdf:parseType="Collection">
	  <owl:Class rdf:about="#Reference" />
	  <owl:Class rdf:about="#Conference" />
	</owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#Date" />
    <rdfs:label xml:lang="en">date</rdfs:label>
